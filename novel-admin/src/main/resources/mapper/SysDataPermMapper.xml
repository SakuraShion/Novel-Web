<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.novel.system.mapper.SysDataPermMapper">

    <resultMap id="BaseResultMap" type="org.example.novel.system.domain.SysDataPerm">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="tableName" column="table_name" jdbcType="VARCHAR"/>
            <result property="moduleName" column="module_name" jdbcType="VARCHAR"/>
            <result property="crlAttrName" column="crl_attr_name" jdbcType="VARCHAR"/>
            <result property="crlColumnName" column="crl_column_name" jdbcType="VARCHAR"/>
            <result property="permCode" column="perm_code" jdbcType="VARCHAR"/>
            <result property="orderNum" column="order_num" jdbcType="INTEGER"/>
            <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
            <result property="gmtModified" column="gmt_modified" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,table_name,
        module_name,crl_attr_name,crl_column_name,
        perm_code,order_num,gmt_create,
        gmt_modified
    </sql>
</mapper>
